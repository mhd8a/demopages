---
---
[
  {% for page in site.pages %}
    {% if page.title != "" %}
      {
        "title"    : "{{ page.title | escape }}",
        "description" : "{{ page.description | strip_html | strip_newlines | escape }}",
        "url" : "{{ site.baseurl }}{{ page.url }}"
      } {% unless forloop.last %},{% endunless %}
    {% endif %}
  {% endfor %}
]
